{
    "name": "jd-dotlogics_tanle/laravel-grapesjs",
    "description": "A package to easily integrate GrapesJS into your laravel proejct.",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": ">=5.6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dotlogics",
            "email": "hello@dotlogics.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dotlogics\\Grapesjs\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dotlogics\\Grapesjs\\GrapesjsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
